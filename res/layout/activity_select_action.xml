<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="co.mide.boilermake.SelectAction" >
 <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/container"
    android:orientation="vertical">  
    <LinearLayout
        android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
		android:layout_weight="1">
		<ImageButton android:id="@+id/act1"
		    android:layout_weight="1"
		    android:onClick="buttonClick"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act2"
		    android:layout_weight="1" 
		    android:onClick="buttonClick"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act3"
		    android:layout_weight="1" 
		    android:onClick="buttonClick"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
	<LinearLayout
	    android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_weight="1">
		<ImageButton android:id="@+id/act4"
		    android:layout_weight="1" 
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:onClick="buttonClick"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act5"
		    android:layout_weight="1" 
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:onClick="buttonClick"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act6"
		    android:layout_weight="1" 
		    android:onClick="buttonClick"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
	<LinearLayout
	    android:orientation="horizontal"
		android:layout_weight="1"
		android:layout_height="wrap_content"
		android:layout_width="match_parent">
		<ImageButton android:id="@+id/act7" 
		    android:contentDescription="@string/empty"
		    android:onClick="buttonClick"
		    android:layout_weight="1"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act8" 
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_weight="1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act9"
		    android:onClick="buttonClick"
		    android:layout_weight="1" 
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
	<LinearLayout
	    android:orientation="horizontal"
		android:layout_weight="1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
		<ImageButton android:id="@+id/act10"
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_weight="1" 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act11" 
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_weight="1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act12" 
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
			android:layout_weight="1"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
	<LinearLayout android:layout_weight="1"
	    android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
		<ImageButton android:id="@+id/act13"
		    android:layout_weight="1" 
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act14"
		    android:onClick="buttonClick"
		    android:src="@drawable/boilermakelogo"
		    android:scaleType="fitCenter"
		    android:layout_weight="1" 
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/act15" 
		    android:layout_weight="1"
		    android:src="@drawable/boilermakelogo"
		    android:onClick="buttonClick"
		    android:scaleType="fitCenter"
		    android:contentDescription="@string/empty"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
</LinearLayout>
<LinearLayout 
    android:layout_height="fill_parent"
    android:clipChildren="false"
   	android:clipToPadding="false"
   	android:cropToPadding="false"
    android:layout_width="fill_parent"
    android:background="#90000000"
    android:orientation="vertical"
    android:gravity="center">
	<RelativeLayout
	    android:layout_width="400dp"
	    android:layout_height="200dp"
	    android:clipChildren = "false"
	    android:clipToPadding="false"
	    android:padding="10dp"
	    android:clickable="true"
	    android:orientation="horizontal"
	    >
	    <ImageButton 
	            android:layout_alignParentTop="true"
	            android:layout_alignParentStart="true"
	            android:layout_alignParentLeft="true"
	            android:clipChildren="false"
		    	android:clipToPadding="false"
		    	android:cropToPadding="false"
		    	android:onClick="close"
	            android:layout_marginStart="-10dp"
	            android:layout_marginLeft="-20dp"
	            android:layout_marginTop="-20dp"
	            android:layout_width="20dp"
	            android:layout_height="20dp"
	            android:contentDescription="@string/empty"
	            android:src="@drawable/close_icon"/>
	    <RealativeLayout 
	        android:layout_centerHorizontal="true" 
	        android:layout_centerVertical="true" 
	        android:layout_width="match_parent"
	        android:layout_height="match_parent">
			<TextView android:id="@+id/confirmExplain"
			    android:layout_width="350dp"
		    	android:layout_height="160dp"/>
			<Button android:layout_width="wrap_content"
			    android:text="@string/yes"
		    	android:layout_height="wrap_content"/>
		</RealativeLayout>
	</RelativeLayout>
    
</LinearLayout>
</RelativeLayout>

